{
  "name": "ol-base-components",
  "version": "2.8.6",
  "private": false,
  "main": "src/package/index.js",
  "bin": {
    "init": "src/api/init.js",
    "run": "src/api/run.js",
    "api": "src/api/api.js",
    "add": "src/bin/add.js",
    "install-vscode": "scripts/install-vscode.js"
  },
  "scripts": {
    "serve": "vue-cli-service serve --no-verify",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "add": "node src/bin/add.js",
    "vscode:build": "node scripts/build-vscode.js",
    "vscode:install": "node scripts/install-vscode.js",
    "postinstall": "node scripts/auto-install.js"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "core-js": "^3.8.3",
    "element-ui": "^2.15.14",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^10.3.0",
    "inquirer": "^8.2.6",
    "swagger-client": "^3.0.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "vue-template-compiler": "^2.6.14",
    "@types/vscode": "^1.60.0"
  },
  "vscode": {
    "extension": {
      "name": "vue-page-generator",
      "displayName": "Vue 页面生成器",
      "description": "配合ol-base-components组件使用, 无需联调的CRUD页面",
      "version": "0.0.4",
      "engines": {
        "vscode": "^1.60.0"
      },
      "activationEvents": [
        "onCommand:vue-generator.createPage"
      ],
      "main": "./src/vscode/extension.js",
      "contributes": {
        "commands": [
          {
            "command": "vue-generator.createPage",
            "title": "生成 Vue 页面",
            "category": "Vue 生成器"
          }
        ],
        "menus": {
          "explorer/context": [
            {
              "command": "vue-generator.createPage",
              "group": "navigation",
              "when": "resourceFolder"
            }
          ]
        }
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "keywords": [
    "ol-base-components"
  ],
  "description": "交互式命令行方式自动生成联调好的CRUD页面",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/time202051/base-component/issues",
    "email": "809949003@qq.com"
  },
  "license": "MIT",
  "author": "lijiapeng"
}
